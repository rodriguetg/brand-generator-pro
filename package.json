    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@upstash/redis": "^1.35.1",
    "autoprefixer": "^10.0.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^8.0.0",
    "framer-motion": "^10.16.4",
    "jspdf": "^3.0.1",
    "next": "14.2.0",
    "next-auth": "^4.24.0",
    "openai": "^4.28.0",
    "postcss": "^8",
    "prisma": "^5.10.0",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "stripe": "^14.7.0",
